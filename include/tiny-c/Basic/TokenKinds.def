// Token, Punctuator, Keyword definitions

#ifndef TOK
#define TOK(ID)
#endif
#ifndef PUNCTUATOR
#define PUNCTUATOR(ID, SP) TOK(ID)
#endif
#ifndef KEYWORD
#define KEYWORD(ID, SP) TOK(kw_##ID)
#endif

TOK(unknown)
TOK(eof)

TOK(ident)

TOK(integer_literal)
TOK(string_literal)

PUNCTUATOR(plus, "+")
PUNCTUATOR(minus, "-")
PUNCTUATOR(star, "*")
PUNCTUATOR(slash, "/")
PUNCTUATOR(comma, ",")
PUNCTUATOR(semi, ";")
PUNCTUATOR(equal, "=")
PUNCTUATOR(l_paren, "(")
PUNCTUATOR(r_paren, ")")
PUNCTUATOR(l_curl, "{")
PUNCTUATOR(r_curl, "}")

KEYWORD(INTEGER, "int")
KEYWORD(RETURN, "return")

#undef KEYWORD
#undef PUNCTUATOR
#undef TOK